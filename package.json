{
  "name": "whatsapp-boleto-bot",
  "version": "1.0.0",
  "description": "Bot do WhatsApp para buscar e enviar boletos via API ZcNet",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:baileys": "cross-env WHATSAPP_PROVIDER=baileys node index.js",
    "start:bot1": "cross-env WHATSAPP_PROVIDER=baileys PORT=3009 BAILEYS_SESSION_ID=bot1 node index.js",
    "start:bot2": "cross-env WHATSAPP_PROVIDER=baileys PORT=3010 BAILEYS_SESSION_ID=bot2 node index.js",
    "start:bot3": "cross-env WHATSAPP_PROVIDER=baileys PORT=3011 BAILEYS_SESSION_ID=bot3 node index.js",
    "dev": "cross-env WHATSAPP_PROVIDER=baileys nodemon index.js --ignore temp_audio --ignore data --ignore tokens --ignore tokens-baileys",
    "list-clients": "node listClients.js",
    "list-all-clients": "node listAllClients.js",
    "test-api": "node listClients.js 5",
    "test-search": "node testSearch.js",
    "test-full-flow": "node testFullFlow.js"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "boleto",
    "api",
    "zcnet"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^7.2.1",
    "@hapi/boom": "^10.0.1",
    "@nlpjs/core": "^5.0.0-alpha.5",
    "@nlpjs/lang-pt": "^5.0.0-alpha.5",
    "@nlpjs/nlp": "^5.0.0-alpha.5",
    "@whiskeysockets/baileys": "^6.7.21",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs": "^0.0.1-security",
    "google-tts-api": "^2.0.2",
    "gtts": "^0.2.1",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "node-tts-api": "^0.0.5",
    "path": "^0.12.7",
    "pino": "^10.1.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "speak-tts": "^2.0.8",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "whatsapp-web.js": "^1.34.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1"
  }
}
